{
  "name": "@icebreakers/monorepo-templates",
  "type": "module",
  "version": "0.1.3",
  "description": "Template assets for icebreaker monorepo scaffolding",
  "author": "ice breaker <1324318532@qq.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sonofmagic/monorepo-template.git",
    "directory": "packages/monorepo-templates"
  },
  "bugs": {
    "url": "https://github.com/sonofmagic/monorepo-template/issues"
  },
  "keywords": [
    "monorepo",
    "template",
    "scaffold"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "assets",
    "assets-data.mjs",
    "dist",
    "template-data.mjs",
    "templates"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "tsdown --watch --sourcemap",
    "build": "tsdown",
    "typecheck": "tsc -p tsconfig.json",
    "sync:assets": "pnpm run build && node scripts/sync-assets.mjs",
    "prepack": "pnpm run build && node scripts/sync-assets.mjs",
    "lint": "eslint ."
  },
  "dependencies": {
    "@inquirer/checkbox": "^5.0.4",
    "@inquirer/input": "^5.0.4",
    "@inquirer/select": "^5.0.4",
    "commander": "^14.0.2",
    "execa": "^9.6.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
