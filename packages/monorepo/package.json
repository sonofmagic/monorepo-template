{
  "name": "@icebreakers/monorepo",
  "type": "module",
  "version": "0.1.1",
  "description": "my monorepo config copyer",
  "author": "sonofmagic",
  "license": "MIT",
  "homepage": "https://github.com/sonofmagic/monorepo-template",
  "keywords": [
    "monorepo"
  ],
  "sideEffects": false,
  "exports": {
    ".": "./src/index.ts"
  },
  "bin": "dev/bin.js",
  "files": [
    "assets",
    "bin",
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch --sourcemap",
    "build": "tsup",
    "test": "vitest run",
    "test:dev": "vitest",
    "release": "pnpm publish",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublish": "tsx scripts/prepublish.ts"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "require": "./dist/index.cjs"
      }
    },
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "bin": "bin/copy.js"
  },
  "dependencies": {
    "fs-extra": "^11.2.0",
    "git-url-parse": "^14.1.0",
    "klaw": "^4.1.0",
    "pathe": "^1.1.2",
    "simple-git": "^3.25.0"
  },
  "devDependencies": {
    "@types/git-url-parse": "^9.0.3"
  }
}
